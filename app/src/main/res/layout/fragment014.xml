<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="@dimen/fab_margin"
    android:background="@color/BlackCool"
    tools:context=".ActivityList.fragment.Frag014_1">

    <Button
        android:id="@+id/btn2anothersFAty"
        style="@style/style_btn_green1"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="@dimen/fab_margin"
        android:text="跳转到另一个fragment" />

    <Button
        android:id="@+id/btn2tabAty"
        style="@style/style_btn_green1"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="@dimen/fab_margin"
        android:text="启动Tabed界面" />

    <Button
        android:id="@+id/btn2navigationAty"
        style="@style/style_btn_green1"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="@dimen/fab_margin"
        android:text="启动带有侧边栏的activity" />

    <Button
        android:id="@+id/btnSend"
        style="@style/style_btn_blue1"
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:layout_marginTop="@dimen/fab_margin"
        android:text="btnSend" />
</LinearLayout>
