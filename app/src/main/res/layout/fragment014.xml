<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/fab_margin"
    tools:context=".fragment.Fragment2Fragment">

    <Button
        android:id="@+id/btn2anothersFAty"
        style="@style/style_btn_green1"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:layout_marginTop="55dp"
        android:text="跳转到另一个fragment"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/btn2tabAty"
        style="@style/style_btn_green1"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:text="启动Tabed界面"
        android:layout_below="@+id/btn2navigationAty"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="92dp"
        android:layout_marginEnd="92dp"
        android:layout_marginTop="62dp" />

    <Button
        android:id="@+id/btn2navigationAty"
        style="@style/style_btn_green1"
        android:layout_width="wrap_content"
        android:layout_height="36dp"
        android:text="启动带有侧边栏的activity"
        android:layout_below="@+id/btn2anothersFAty"
        android:layout_toLeftOf="@+id/btn2tabAty"
        android:layout_toStartOf="@+id/btn2tabAty" />

</RelativeLayout>
